<% if user_signed_in? %>
  <h1 class="mb-5">Files imported</h1>
  <% if @files_imported.exists? %>
    <%= render partial: 'log_files/log_file', collection: @files_imported, as: :log_file %>

    <%= pagy_bootstrap_nav(@pagy).html_safe %>
  <% else %>
    <h4 class="text-muted">Empty</h4>
  <% end %>
<% else %>
  <h1>Welcome</h1>
<% end %>
